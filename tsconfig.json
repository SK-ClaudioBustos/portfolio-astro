{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@type/*": [
        "./src/types/*"
      ],
      "@icons/*": [
        "./src/icons/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@sections/*": [
        "./src/components/sections/*"
      ],
      "@i18n/*": [
        "./src/i18n/*"
      ],
      "@img/*": [
        "./src/img/*"
      ]
    },
    "moduleResolution": "node",
    "module": "ESNext",
    "isolatedModules": true,
    "strict": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ]
}